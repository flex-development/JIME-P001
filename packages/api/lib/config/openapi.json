  "paths": {
    "/collections": {
      "get": {
        "summary": "Find collections",
        "description": "Returns an array of collection listing resource objects",
        "operationId": "find-collections",
        "tags": ["Collections"],
        "parameters": [
          {
            "name": "collection_id",
            "in": "query",
            "description": "Find collection listing by ID",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "fields",
            "in": "query",
            "description": "Comma-separated list of fields to include. By default the `collection_id` and `objectID` fields will be included. To request all fields, use the value `*`.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "hitsPerPage",
            "in": "query",
            "description": "Number of results per page",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "length",
            "in": "query",
            "description": "Result limit (used only with offset)",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "objectID",
            "in": "query",
            "description": "Find resource by index object ID (collection listing handle)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "description": "Offset of the first result to return",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Specify the page to retrieve",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "text",
            "in": "query",
            "description": "Text to search in index",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Shopify.ICollectionListing"
                  },
                  "type": "array"
                }
              }
            }
          },
          "400": {
            "description": "BadRequest",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errors.BadRequest"
                }
              }
            }
          },
          "500": {
            "description": "GeneralError",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errors.GeneralError"
                }
              }
            }
          },
          "502": {
            "description": "BadGateway",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Errors.BadGateway"
                }
              }
            }
          }
        }
      }
    },
    "/collections/[objectID]": {
      "get": {
        "summary": "Get collection by handle",
        "description": "Retrieve a collection listing resource by handle",
        "operationId": "get-collection-by-handle",
        "tags": ["Collections"],
        "parameters": [
          {
            "name": "fields",
            "in": "query",
            "description": "Comma-separated list of fields to include. By default the `collection_id` and `objectID` fields will be included. To request all fields, use the value `*`.",
