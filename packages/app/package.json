{
  "name": "@flex-development/morenaskustomz",
  "version": "1.0.0",
  "description": "Shopify storefront powered by Next.js",
  "keywords": [
    "nextjs",
    "react",
    "shopify",
    "vercel",
    "typescript"
  ],
  "homepage": "https://morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/app"
  },
  "scripts": {
    "kill:firebase:db": "kill $(lsof -ti:5000) || true",
    "kill:firebase:ui": "kill $(lsof -ti:4000) || true",
    "kill:firebase": "run-p kill:firebase:*",
    "kill:d": "kill $(lsof -ti:3001) || true",
    "predev": "yarn kill:d; vc env pull .env.development.local --yes",
    "dev": "next dev -p 3001",
    "mock:db": "firebase emulators:start --only database,ui",
    "pretest": "yarn kill:firebase",
    "test": "firebase emulators:exec \"yarn test:run\" --only database",
    "test:quiet": "yarn test || true",
    "test:run": "jest --detectOpenHandles --passWithNoTests",
    "prestart": "rm .env.development.local || true; yarn build",
    "start": "next start -p 9000",
    "prebuild": "rm -rf .next; generate-robotstxt public/robots.txt",
    "build": "next build --debug",
    "prevercel-build": "lerna run prepare; lerna link",
    "vercel-build": "yarn build && node ./scripts/delete-source-maps"
  },
  "dependencies": {
    "@baggie/string": "^1.1.0",
    "@flex-development/kustomzcore": "*",
    "@flex-development/kustomzdesign": "*",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/plugin-google-analytics": "^10.0.5",
    "@next/plugin-sentry": "^10.0.5",
    "@sentry/browser": "^6.0.2",
    "@sentry/integrations": "^6.0.2",
    "@sentry/node": "^6.0.2",
    "@sentry/webpack-plugin": "^1.14.0",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "axios": "^0.19.2",
    "axios-rate-limit": "^1.3.0",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.2.0",
    "fs-extra": "^9.0.1",
    "generate-robotstxt": "^8.0.3",
    "inspectpack": "^4.5.2",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.15",
    "next": "^10.0.5",
    "next-transpile-modules": "^6.0.0",
    "pino": "^6.11.0",
    "pino-pretty": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sitemap": "^6.3.3",
    "swr": "^0.3.9",
    "ua-parser-js": "^0.7.23",
    "util": "^0.12.3",
    "webfontloader": "^1.6.28",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-tap-done": "file:plugins/webpack-tap-done"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.0.9",
    "@types/debug": "^4.1.5",
    "@types/gtag.js": "^0.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pino": "^6.3.5",
    "@types/ua-parser-js": "^0.7.35",
    "@types/webfontloader": "^1.6.32",
    "ts-jest": "^26.4.1"
  },
  "required-env": [
    "ANALYZE",
    "API_URL",
    "DEBUG",
    "DEBUG_COLORS",
    "FIREBASE_API_KEY",
    "FIREBASE_APP_ID",
    "FIREBASE_PROJECT_ID",
    "FIREBASE_MESSAGING_SENDER_ID",
    "GA_TRACKING_ID",
    "GOOGLE_SITE_VERIFICATION",
    "NODE_ENV",
    "NPM_RC",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_RELEASE",
    "SHOPIFY_API_VERSION",
    "SHOPIFY_DOMAIN",
    "SITE_NAME",
    "SITE_URL",
    "TYPEKIT_ID",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_GITHUB_COMMIT_SHA",
    "VERCEL_URL"
  ]
}
