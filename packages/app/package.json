{
  "name": "@flex-development/morenaskustomz",
  "version": "1.0.0",
  "description": "Shopify storefront powered by Next.js",
  "keywords": [
    "nextjs",
    "react",
    "shopify",
    "vercel",
    "typescript"
  ],
  "homepage": "https://morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/app"
  },
  "scripts": {
    "kill:firebase:db": "kill $(lsof -ti:5000) || true",
    "kill:firebase:ui": "kill $(lsof -ti:4000) || true",
    "kill:firebase": "run-p kill:firebase:*",
    "kill:d": "kill $(lsof -ti:3001) || true",
    "predev": "yarn kill:d; NODE_ENV=development vc env pull .env.local --yes",
    "dev": "next dev -p 3001",
    "mock:db": "firebase emulators:start --only database,ui",
    "pretest": "yarn kill:firebase",
    "test": "firebase emulators:exec \"yarn test:run\" --only database",
    "test:quiet": "yarn test || true",
    "test:run": "jest --detectOpenHandles --passWithNoTests",
    "prestart": "NODE_ENV=production",
    "start": "next start",
    "prebuild": "NODE_ENV=production generate-robotstxt public/robots.txt",
    "build": "next build",
    "prenow-build": "lerna run prepare; lerna link",
    "now-build": "yarn build"
  },
  "dependencies": {
    "@flex-development/kustomzcore": "*",
    "@flex-development/kustomzdesign": "*",
    "axios": "^0.19.2",
    "axios-rate-limit": "^1.3.0",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.2.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reactfire": "^3.0.0-rc.0",
    "sass": "^1.26.10",
    "sitemap": "^6.3.3",
    "slugify": "^1.4.5",
    "string-strip-html": "^7.0.3",
    "styled-components": "^5.2.0",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.0.9",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-styled-components": "^1.11.1",
    "generate-robotstxt": "^8.0.3"
  },
  "peerDependencies": {}
}
