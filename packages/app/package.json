{
  "name": "@flex-development/morenaskustomz",
  "version": "1.0.0",
  "description": "Shopify storefront powered by Next.js",
  "keywords": [],
  "homepage": "https://morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/app"
  },
  "scripts": {
    "predev": "vc env pull .env.development.local --yes",
    "dev": "bash ./scripts/next-dev.sh",
    "test": "BABEL_ENV=jest jest --detectOpenHandles --passWithNoTests",
    "test:quiet": "yarn test || true",
    "build": "bash ./scripts/next-build.sh",
    "prevercel-build": "lerna run prepare; lerna link",
    "vercel-build": "yarn build && node ./scripts/js/delete-source-maps",
    "prestart": "yarn build",
    "start": "next start -p 9000"
  },
  "dependencies": {
    "@flex-development/kustomzdesign": "*",
    "@flex-development/webpack-tap-done": "latest",
    "@mdx-js/react": "latest",
    "@next/plugin-sentry": "canary",
    "@sentry/browser": "latest",
    "@sentry/integrations": "latest",
    "@sentry/node": "latest",
    "@sentry/webpack-plugin": "latest",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "fs-extra": "latest",
    "ga-measurement-protocol": "latest",
    "generate-robotstxt": "latest",
    "inspectpack": "latest",
    "lodash-es": "latest",
    "nanoid": "latest",
    "next": "canary",
    "next-transpile-modules": "latest",
    "pino": "latest",
    "pino-pretty": "latest",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sitemap": "latest",
    "tslib": "latest",
    "ua-parser-js": "latest",
    "urijs": "latest",
    "util": "latest",
    "webpack": "5.21.2"
  },
  "devDependencies": {
    "@types/debug": "latest",
    "@types/feathersjs__errors": "latest",
    "@types/gtag.js": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/lodash": "latest",
    "@types/pino": "latest",
    "@types/ua-parser-js": "latest",
    "@types/uuid": "latest",
    "@types/webfontloader": "latest",
    "webpack-bundle-analyzer": "latest"
  },
  "required-env": [
    "API_URL",
    "DEBUG",
    "DEBUG_COLORS",
    "GA_ENABLED",
    "GA_TRACKING_ID",
    "GOOGLE_SITE_VERIFICATION",
    "NODE_ENV",
    "NPM_RC",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_RELEASE",
    "SITE_NAME",
    "SITE_URL",
    "TYPEKIT_ID",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_GIT_COMMIT_REF",
    "VERCEL_GIT_COMMIT_SHA",
    "VERCEL_URL"
  ]
}
