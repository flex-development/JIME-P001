    "@types/shopify-buy": "^2.10.3",
    "@types/styled-components": "^5.1.4",
    "@types/urijs": "^1.19.9",
    "@types/validator": "^13.1.0",
    "@vercel/node": "^1.8.1",
    "axios": "^0.19.2",
    "axios-rate-limit": "^1.3.0",
    "class-transformer": "^0.3.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.12.2",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
