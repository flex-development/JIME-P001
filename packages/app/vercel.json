{
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": false
  },
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 10
    },
    "src/pages/api/sitemap.ts": {
      "includeFiles": "src/pages/**"
    }
  },
  "redirects": [
    {
      "source": "/checkouts/:permalink",
      "destination": "https://morenaskustomz.myshopify.com/cart/:permalink",
      "permanent": true
    },
    {
      "source": "/instagram",
      "destination": "https://instagram.com/morenaskustomz",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index"
    },
    {
      "source": "/admin",
      "destination": "/api/admin/login"
    },
    {
      "source": "/products",
      "destination": "/collections/all-products"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
