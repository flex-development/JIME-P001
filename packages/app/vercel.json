{
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": false
  },
  "redirects": [
    {
      "source": "/admin",
      "destination": "https://morenaskustomz.myshopify.com/admin",
      "permanent": true
    },
    {
      "source": "/checkouts/:permalink",
      "destination": "https://morenaskustomz.myshopify.com/cart/:permalink",
      "permanent": true
    },
    {
      "source": "/instagram",
      "destination": "https://instagram.com/morenaskustomz",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/pages/index",
      "destination": "/"
    },
    {
      "source": "/pages/:handle*",
      "destination": "/:handle*"
    },
    {
      "source": "/products",
      "destination": "/collections/all-products"
    },
    {
      "source": "/products/:handle*",
      "destination": "/collections/all-products/products/:handle*"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    }
  ],
  "headers": [
    {
      "source": "/404",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, always"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
