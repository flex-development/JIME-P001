{
  "name": "@flex-development/kustomz",
  "version": "1.0.0",
  "description": "Storybook design system for Morenas Kustomz",
  "keywords": [
    "design system",
    "component library",
    "react",
    "sass",
    "storybook",
    "typescript"
  ],
  "homepage": "https://design.morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/system"
  },
  "publishConfig": {
    "directory": "dist",
    "registry": "https://npm.pkg.github.com/"
  },
  "source": "src/index.ts",
  "main": "dist/index.ts",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "predev": "cross-env NODE_ENV=development",
    "dev:types": "lerna run dev --scope @flex-development/kustomtypez",
    "dev:storybook": "start-storybook -p 3000 -s ../app/public --ci",
    "dev:storybook:docs": "yarn dev:storybook --docs",
    "dev": "run-p dev:*",
    "pretest": "cross-env NODE_ENV=test",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prebuild:bundle": "rm -rf dist && mkdir dist dist/style",
    "prebuild:storybook": "rm -rf public && mkdir public",
    "build:bundle": "microbundle --tsconfig tsconfig.prod.json --jsx React.createElement",
    "build:storybook": "build-storybook -o public",
    "postbuild:bundle": "shx cp -R src/theme/* dist/theme",
    "postbuild:storybook": "shx cp -R ../app/public/assets/** public",
    "build": "run-p build:*"
  },
  "dependencies": {
    "@flex-development/kustomtypez": "*",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.158",
    "bootstrap": "^5.0.0-alpha1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.19",
    "react-hanger": "^2.2.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-backgrounds": "^6.0.21",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-controls": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/storybook__react": "^5.2.1",
    "@whitespace/storybook-addon-html": "^1.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "microbundle": "^0.12.0-next.9",
    "next": "^9.5.1",
    "postcss": "^7.0.32",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0"
  },
  "peerDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
