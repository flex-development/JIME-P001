  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/system"
  },
  "publishConfig": {
    "directory": "src"
  },
  "main": "src/index.ts",
  "scripts": {
    "predev": "vc env pull --yes && yarn test:quiet",
    "dev:storybook": "start-storybook -p 3000 -s ../app/public --ci",
    "dev:storybook:docs": "yarn dev:storybook --docs",
    "dev": "run-p dev:*",
    "test": "jest --detectOpenHandles --passWithNoTests --json --outputFile=__tests__/jest-test-results.json",
    "test:quiet": "yarn test || true",
    "prebuild": "yarn test:quiet && rm -rf public && mkdir public",
    "build": "build-storybook -o public",
    "postbuild": "shx cp -R ../app/public/assets public"
  },
