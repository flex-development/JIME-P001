{
  "name": "@flex-development/kustomzdesign",
  "version": "1.0.0",
  "description": "Storybook design system for Morena's Kustomz",
  "keywords": [
    "design system",
    "component library",
    "react",
    "sass",
    "storybook",
    "typescript"
  ],
  "homepage": "https://design.morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/system"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "predev": "vc env pull --yes && yarn test:quiet",
    "dev:kill": "kill $(lsof -ti:3000) || true",
    "dev:storybook": "start-storybook -p 3000 -s ../app/public --ci",
    "dev:storybook:docs": "yarn dev:storybook --docs",
    "dev": "run-p dev:*",
    "test": "BABEL_ENV=jest jest --detectOpenHandles --passWithNoTests --json --outputFile=__tests__/jest-test-results.json",
    "test:quiet": "yarn test || true",
    "start": "yarn predev; NODE_ENV=production run-p dev:*",
    "prebuild": "yarn test:quiet && rm -rf public && mkdir public",
    "build": "NODE_ENV=production build-storybook -o public",
    "vercel-build": "lerna run prepare --scope @flex-development/kustomzcore; lerna link; yarn build",
    "precompile:ts": "rm -rf ./dist",
    "compile:ts": "babel --config-file ./babel.config.prod.js src -d dist --extensions '.ts,.tsx'",
    "postcompile:ts": "ttsc -p tsconfig.prod.json",
    "compile:scss": "webpack -c webpack.prod.js",
    "postcompile:scss": "rm ./dist/css.js",
    "compile": "NODE_ENV=production run-s compile:*",
    "prepare": "if-env SKIP_PREPARE=true && exit 0 || yarn compile"
  },
  "dependencies": {
    "@react-spring/web": "9.0.0-rc.3",
    "@shopify/polaris-icons": "^4.1.0",
    "classnames": "^2.2.6",
    "react-hanger": "^2.2.1",
    "react-use": "^15.3.4",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-a11y": "^6.1.5",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.1.5",
    "@storybook/addon-jest": "^6.1.5",
    "@storybook/addons": "^6.1.5",
    "@storybook/api": "^6.1.5",
    "@storybook/cli": "^6.1.5",
    "@storybook/react": "^6.1.5",
    "@testing-library/dom": "^7.24.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.3.0",
    "@types/classnames": "^2.2.10",
    "@types/mdx-js__react": "^1.5.3",
    "@whitespace/storybook-addon-html": "^2.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.3.0",
    "cssnano-simple": "^1.2.1",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.5",
    "storybook-addon-performance": "^0.13.0",
    "storybook-mobile": "^0.1.28",
    "style-loader": "^1.2.1",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "@flex-development/kustomzcore": "*",
    "@mdx-js/react": "^1.6.22",
    "@types/mdx-js__react": "^1.5.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependenciesMeta": {
    "@mdx-js/react": {
      "optional": true
    },
    "@types/mdx-js__react": {
      "optional": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
