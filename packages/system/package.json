{
  "name": "@flex-development/kustomzdesign",
  "version": "1.0.0",
  "description": "Storybook design system for Morena's Kustomz",
  "keywords": [
    "design system",
    "component library",
    "react",
    "sass",
    "storybook",
    "typescript"
  ],
  "homepage": "https://design.morenaskustomz.com",
  "bugs": {
    "url": "https://github.com/flex-development/JIME-P001/issues",
    "email": "developers@flexdevelopment.llc"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Lexus Drumgold",
    "email": "lex@flexdevelopment.llc",
    "url": "https://flexdevelopment.llc"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/flex-development/JIME-P001.git",
    "directory": "packages/system"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/kustomzdesign.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "predev": "vc env pull --yes && yarn test:quiet",
    "dev:kill": "kill $(lsof -ti:3000) || true",
    "dev:storybook": "start-storybook -p 3000 -s ../app/public --ci",
    "dev:storybook:docs": "yarn dev:storybook --docs",
    "dev": "run-p dev:*",
    "test": "jest --detectOpenHandles --passWithNoTests --json --outputFile=__tests__/jest-test-results.json",
    "test:quiet": "yarn test || true",
    "start": "NODE_ENV=production yarn dev",
    "prebuild": "yarn test:quiet && rm -rf public && mkdir public",
    "build": "build-storybook -o public",
    "postbuild": "shx cp -R ../app/public/assets public",
    "prenow-build": "NODE_ENV=production lerna run prepare; lerna link",
    "now-build": "yarn build",
    "bundle:js": "tsdx build --tsconfig tsconfig.prod.json --transpileOnly",
    "postbundle:js": "shx cp -R ./src/types ./dist/types",
    "bundle:styles": "webpack -c webpack.prod.js",
    "postbundle:styles": "shx cp -R ./src/scss ./dist/scss; rm ./dist/css.js",
    "size": "size-limit",
    "prepare": "NODE_ENV=production run-s bundle:*",
    "prepublishOnly": "yarn size"
  },
  "dependencies": {
    "@shopify/polaris-icons": "^4.1.0",
    "@shopify/theme-images": "^4.1.0",
    "@types/mdx-js__react": "^1.5.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "mdx-scoped-runtime": "^0.7.5",
    "react-hanger": "^2.2.1",
    "react-spring": "^9.0.0-rc.3",
    "react-use": "^15.3.4",
    "react-use-gesture": "^9.0.0-beta.14",
    "react-uuid": "^1.0.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^4.9.1",
    "@storybook/addon-a11y": "^6.1.9",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-jest": "^6.1.9",
    "@storybook/addons": "^6.1.9",
    "@storybook/api": "^6.1.9",
    "@storybook/cli": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "@testing-library/dom": "^7.24.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.3.0",
    "@types/bootstrap": "^4.5.0",
    "@types/classnames": "^2.2.10",
    "@types/mdx-js__react": "^1.5.3",
    "@types/webfontloader": "^1.6.32",
    "@whitespace/storybook-addon-html": "^2.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^1.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "storybook-addon-performance": "^0.13.0",
    "storybook-mobile": "^0.1.28",
    "style-loader": "^1.2.1",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "@flex-development/kustomzcore": "*",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/kustomzdesign.cjs.production.min.js",
      "limit": "500 KB"
    },
    {
      "path": "dist/kustomzdesign.esm.js",
      "limit": "500 KB"
    }
  ]
}