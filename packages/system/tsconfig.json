{
  "awesomeTypescriptLoaderOptions": {
    "babelCore": "@babel/core",
    "babelOptions": {
      "babelrc": true
    },
    "useBabel": true,
    "useCache": true
  },
  "compilerOptions": {
    "checkJs": false,
    "downlevelIteration": true,
    "isolatedModules": false,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "sourceMap": false,
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "extends": "../../tsconfig.json",
  "include": ["__tests__", ".storybook", "src", "jest.setup.ts"]
}
